#include <iostream>
using namespace std;

int main()
{
  int num;
  char symbol;
  int sum = 0;
  bool cont = true;
  cin >> num;
  sum += num;
  for (int i = 1; cont; i++)
  {
    if (i % 2 == 0)
    {
        cin >> num;
        if (symbol == '+')
          sum += num;
        else if (symbol == '-')
          sum -= num;
        else if (symbol == ';')
        {
          cout << sum << endl;
          sum = 0;
          sum += num;
        }
    }
    else if (!(cin >> symbol))
      cont = false;
  }
}
