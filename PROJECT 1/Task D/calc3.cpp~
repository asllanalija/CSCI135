#include <iostream>
using namespace std;

int main()
{
  int num;
  char symbol;
  int sum = 0;
  bool cont = true;
  bool pos = true;
  while (cont)
  {
    if (cin >> num)
    {
      cin >> symbol;
      if (symbol == '^')
      {
        if (pos)
          sum += num*num;
        else
          sum -= num*num;
        cin >> symbol;
      }
      else
      {
        if (pos)
          sum += num;
        else
          sum-= num;
      }
      if (symbol == '+')
        pos = true;
      else if (symbol == '-')
        pos = false;
      else if (symbol == ';')
      {
        cout << sum << endl;
        sum = 0;
        symbol = '+';
        pos = true;
      }
    }
    else
    {
      cont = false;
    }
  }
}
