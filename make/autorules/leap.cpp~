Prints whether or not the given year is a leap year or not.
*/
#include <iostream>
using namespace std;
int main() 
{
  //initialize year variable
  int year;
  string com = "Common year";
  string leap = "Leap year";

  // take in year from user
  cout << "Enter year: ";
  cin >> year;
  cout << endl;

  // Checks if year is a leap year
  if (year % 4 != 0)
    cout << com;
  else if (year % 100 != 0)
    cout << leap;
  else if (year % 400 != 0)
    cout << com;
  else
    cout << leap;

  return 0;
}