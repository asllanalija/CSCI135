#include <iostream>

template <class T>
class Three
{
  public:
    Three (T a, T b, T c);
    void printem();
  private:
    T data[3];
};

Three::Three(T a, T b, T c)
{
    data[0] = a;
    data[1] = b;
    data[2] = c;
}

void Three::printem()
{
    for (int i = 0; i < 3; i++)
        std::cout << data[i];
    std::cout << '\n'
}
